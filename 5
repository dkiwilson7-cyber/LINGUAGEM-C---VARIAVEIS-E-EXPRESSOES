/*  5. Leia um número real e imprima a quinta parte deste número.  */



#include <stdio.h>
#include <stdlib.h>

int main()
{
    float Num, Qui;

    printf("Digite um numero real qualquer: \n");
    scanf("%f", &Num);
    Qui = (Num/5.0);

    printf("A quinta parte do numero digitado vale: %.2f \n", Qui);

    system("pause");
    return 0;
}
