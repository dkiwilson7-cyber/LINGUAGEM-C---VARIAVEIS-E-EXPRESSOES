/*  4. Leia um número real e imprima o resultado do quadrado desse número.  */



#include <stdio.h>
#include <stdlib.h>

int main()
{
    float Num, Qua;

    printf("Digite um numero real qualquer: \n");
    scanf("%f", &Num);
    Qua = Num*Num;

    printf("O quadrado desse numero eh: %.2f \n", Qua);

    system("pause");
    return 0;
}
